                    <td>$支払済</td>
                    <td>$未配送</td>
                    <td>$100</td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
          <div class="mypage-row-child-right">
            <h3>アカウントの詳細</h3>
            <form method="post" action="/manage/checkaddress">
              <button class="btn-cart btn-blue">
                <input class="btn-checkaddress" type="submit" value="住所を確認する">
              </button>
            </form>
          </div>
        </div>
      </div>
    </div>
  </main>

  <footer>
    <hr>
    <div class="footer-area">
      <div class="content-area">
        <img class="appeal" src="/images/logo/LOGO.png">
        <div class="category">
          <% if (typeof category !='string' ) { %>
            <% category.forEach(function(val) { %>

<body>
  <header>
    <div class="header-wrapper">
      <div class="header-flex">
        <div class="header-icon hamburger">
          <div></div>
          <div></div>
          <div></div>
        </div>
      </div>
      <div class="header-logo">
        <a href="/">
          <div class="gallery-cell">
            <img class="forpc header-image" src="/images/logo/LOGO.png" alt="">
            <img class="forsp header-image" src="/images/logo/LOGO.png"
              srcset="/images/logo/LOGO.png 1x,/images/logo/LOGO@2x.png 2x,/images/logo/LOGO@3x.png 3x" alt="">
          </div>
        </a>
      </div>
      <div class="header-iconarea">
        <div class="header-icon forpc">
          <img src="/images/icon/search.png">
        </div>
        <div class="header-icon">
          <i class="fa-solid fa-heart"></i>
        </div>
        <div class="header-icon">
          <i class="fa-solid fa-user"></i>
        </div>
        <div class="header-icon">
          <i class="fa-solid fa-cart-shopping"></i>
        </div>
      </div>
    </div>
    <div class="search-box forsp">
      <input type="text" name="search" placeholder="検索" class="textbox">
    </div>
  </header>

  <main>
    <nav class="navigation forpc">
      <ul>
        <% if (typeof category !='string' ) { %>
          <% category.forEach(function(val) { %>
            <li>
              <a href="/category/<%= val.id %>">
                <%= val.categoryname %>
              </a>
            </li>
            <% }) %>
              <% } %>
      </ul>
    </nav>
    <div class="main-breadcrumb-wrapper">
      <nav>
        <ol class="breadcrumb">
